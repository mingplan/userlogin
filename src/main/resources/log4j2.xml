<?xml version="1.0" encoding="UTF-8"?>
<configuration status="ALL">
	<appenders>
		<File name="log" fileName="./log/userlogin.log" append="true">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%class{36}.%M]- %msg%xEx%n" />
		</File>
	</appenders>
	<loggers>
		<root level="ALL">
			<appender-ref ref="log" />
		</root>
	</loggers>
</configuration>